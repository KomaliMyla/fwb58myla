extends layout

block content
  h1= title
  - const Chairs = ['Chair_type','Cost','Color']
  - let results;  
  script.
    function buy(c){
      alert('Purchased Item: ' + c)
   } 
  div(class='container-fluid')
    div(class='row')
      each results, index in Chairs 
        div(class='col-sm-2' onclick='buy('+index+')') #{results} 
